page {
  background-color: #ebe7e7;
}
.login {
  height: 360rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.login .my {
  text-align: center;
  width: 100%;
  height: 100%;
  position: relative;
}
.login .my .myImg {
  position: absolute;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  overflow: hidden;
  left: 50%;
  transform: translateX(-50%);
  top: 80rpx;
}
.login .my .myImg image {
  width: 100%;
  height: 100%;
}
.login .my .nickName {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 240rpx;
  color: #fff;
}
.login .my .my-bg {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  filter: blur(14rpx);
}
.login .my .my-bg image {
  width: 100%;
  height: 100%;
}
.cont {
  padding: 28rpx 0;
  background-color: #fff;
  margin: 24rpx 0;
  display: flex;
  justify-content: space-around;
}
.cont .contItem {
  text-align: center;
}
.cont .contItem .iconfont {
  font-size: 60rpx;
}
.cont .contItem .p {
  font-size: 32rpx;
}
.center {
  height: 460rpx;
  background-color: #fff;
}
.center .cenLis {
  padding: 24rpx 0;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #ccc;
}
.center .cenLis .left {
  display: flex;
  align-items: center;
  margin-left: 15rpx;
}
.center .cenLis .left .iconfont {
  font-size: 52rpx;
}
.center .cenLis .left view {
  margin-left: 20rpx;
  font-size: 32rpx;
}
.center .cenLis .ri {
  margin-right: 20rpx;
  font-size: 32rpx;
}
